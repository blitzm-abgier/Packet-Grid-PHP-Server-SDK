{
  "name": "packet-grid/php-server",
  "description": "The PacketGrid PHP SDK for servers",
  "keywords": [
    "packetgrid",
    "packet",
    "grid",
    "openapitools",
    "openapi-generator",
    "openapi",
    "php",
    "sdk",
    "rest",
    "api"
  ],
  "authors": [
    {
      "name": "Abgier Avraha",
      "email": "abgier@blitzm.com"
    }
  ],
  "homepage": "https://packetgrid.io/",
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.3.0",
    "packet-grid/php-server-sdk": "@dev"
  },
  "repositories": [
    {
      "type": "path",
      "url": "sdk/"
    }
  ],
  "autoload": {
    "psr-4": {
      "OpenAPI\\Client\\" : "sdk/lib/",
      "PacketGridSDK\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  }
}
